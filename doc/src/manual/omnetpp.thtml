  hr.pgbr { color: #f5f5f5; margin-top: 40px; margin-bottom: 10px; }
  p.subheading { color: #1A41A8; margin-top: 30px; font-weight: bold; }
  pre { margin: 4px 8px 4px 2px; padding: 8px 10px; background:#fffff4; border: 1px solid #dddddd; }
