#
# Global definitions
#
!include ..\..\configuser.vc

#
# Local setup
#
!if "$(TKENV_EMBED_TCL)"=="yes"
TKENV_EMBED_SUFFIX=-e
!else
TKENV_EMBED_SUFFIX=
!endif

!if "$(LIB_SUFFIX)"==".dll"
CFLAGS=$(CFLAGS) /DWIN32_DLL
!endif

#
# Local definitions
#
COPTS=$(CFLAGS) /I "$(OMNETPP_INCL_DIR)" /I "../envir" $(TK_CFLAGS) /DBUILDING_TKENV
OBJS = tkapp$(TKENV_EMBED_SUFFIX).obj tkcmd.obj tklib.obj objinsp.obj modinsp.obj statinsp.obj inspect.obj arrow.obj
LIBS = $(OMNETPP_SRC_DIR)/sim/sim_std.ilib $(OMNETPP_SRC_DIR)/envir/envir.ilib $(TK_LIB_DIR)/tcl82.lib $(TK_LIB_DIR)/tk82.lib

#
# Targets
#
default: $(TKENV_IF_POSSIBLE)

tkenv : tkenv$(LIB_SUFFIX)

tkenv.lib : $(OBJS)
	$(AR) /nologo /out:tkenv.lib $(OBJS)
        copy tkenv.lib $(OMNETPP_LIB_DIR:/=\)

tkenv.dll : $(OBJS) $(LIBS)
	$(SHLIB_LD) /nologo /out:tkenv.dll /implib:tkenv.ilib /subsystem:console /def:tkenv.def $(OBJS) $(LIBS)
        copy tkenv.dll $(OMNETPP_LIB_DIR:/=\)
        copy tkenv.ilib $(OMNETPP_LIB_DIR:/=\)

tkapp.obj : tkapp.cc
	$(CXX) /DOMNETPP_TKENV_DIR="\"$(OMNETPP_TKENV_DIR)\"" /DOMNETPP_BITMAP_PATH="\"$(OMNETPP_BITMAP_PATH)\"" $(COPTS) /c /Tp tkapp.cc

tkapp-e.obj : tkapp.cc  tclcode.cc
	$(CXX) /Fo"tkapp-e.obj" /DOMNETPP_BITMAP_PATH="\"$(OMNETPP_BITMAP_PATH)\"" $(COPTS) /c /Tp tkapp.cc

tkcmd.obj : tkcmd.cc
	$(CXX) $(COPTS) /c /Tp tkcmd.cc

tklib.obj : tklib.cc
	$(CXX) $(COPTS) /c /Tp tklib.cc

objinsp.obj: objinsp.cc
	$(CXX) $(COPTS) /c /Tp objinsp.cc

modinsp.obj: modinsp.cc
	$(CXX) $(COPTS) /c /Tp modinsp.cc

statinsp.obj: statinsp.cc
	$(CXX) $(COPTS) /c /Tp statinsp.cc

inspect.obj: inspect.cc
	$(CXX) $(COPTS) /c /Tp inspect.cc

arrow.obj: arrow.cc arrow.h
	$(CXX) $(COPTS) /c /Tp arrow.cc

tclcode.cc: *.tcl
	..\utils\tcl2c tclcode.cc *.tcl

missing-library:
	@ echo '  *** No Tk installation found -- skipping Tkenv'

depend:
	$(MAKEDEPEND) -I../../include -fMakefile.vc -- *.cc

clean:
	-del tclcode.cc *.obj tkenv.lib tkenv.dll tkenv.ilib tkenv.exp 2> nul
	-del *.ilk *.idb *.pdb 2> nul

