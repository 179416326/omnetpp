#
# Definitions to be customized by 'configure'
#
CXX=@CXX@
AR=@AR@
SHLIB_LD=@SHLIB_LD@
RANLIB=@RANLIB@
MAKEDEPEND=@MAKEDEPEND@
PERL=@PERL@

CFLAGS=@CFLAGS@

OMNETPP_INCL_DIR=@OMNETPP_INCL_DIR@
OMNETPP_LIB_DIR=@OMNETPP_LIB_DIR@

OMNETPP_VERSION=@OMNETPP_VERSION@
OMNETPP_EDITION=@OMNETPP_EDITION@

# LIB_SUFFIX is either A_LIB_SUFFIX or SO_LIB_SUFFIX
LIB_SUFFIX=@LIB_SUFFIX@

A_LIB_SUFFIX=@A_LIB_SUFFIX@
SO_LIB_SUFFIX=@SO_LIB_SUFFIX@

#
# Local definitions
#
COPTS=$(CFLAGS) -I. -I../common -I$(OMNETPP_INCL_DIR)

OBJS= geometry.o graphcomponent.o heapembedding.o startreeembedding.o forcedirectedembedding.o forcedirectedparametersbase.o forcedirectedparameters.o
LIBS= -llayout -lcommon

#
# Automatic rules
#
.SUFFIXES : .cc

.cc.o:
	$(CXX) -c $(COPTS) -o $@ $<

#
# Targets
#
all: liblayout$(LIB_SUFFIX) layouttool

liblayout$(A_LIB_SUFFIX) : $(OBJS)
	$(AR) liblayout$(A_LIB_SUFFIX) $(OBJS)
	$(RANLIB) liblayout$(A_LIB_SUFFIX)
	../utils/install-lib liblayout$(A_LIB_SUFFIX).$(OMNETPP_VERSION) $(OMNETPP_LIB_DIR) liblayout$(A_LIB_SUFFIX)
liblayout$(SO_LIB_SUFFIX) : $(OBJS)
	$(SHLIB_LD) -o liblayout$(SO_LIB_SUFFIX) $(OBJS)
	../utils/install-lib liblayout$(SO_LIB_SUFFIX).$(OMNETPP_VERSION) $(OMNETPP_LIB_DIR) liblayout$(SO_LIB_SUFFIX)

layouttool : layouttool.o libeventlog$(LIB_SUFFIX)
	$(CXX) $(LDFLAGS) -o layouttool layouttool.o -L$(OMNETPP_LIB_DIR) $(LIBS)
	../utils/install-prog layouttool $(OMNETPP_BIN_DIR)

depend:
	$(MAKEDEPEND) -I. -I../common -- *.cc
	$(MAKEDEPEND) -I. -I../common -fMakefile.in -- *.cc

clean:
	rm -f *.o liblayout$(A_LIB_SUFFIX) liblayout$(SO_LIB_SUFFIX)

