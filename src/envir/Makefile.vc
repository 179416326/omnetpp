#
# Global definitions
#
!include ..\..\configuser.vc

# Make sure that output locations are set
!if "$(OMNETPP_BIN_DIR)"==""
!error OMNETPP_BIN_DIR must be correctly set
!endif
!if "$(OMNETPP_OUT_DIR)"==""
!error OMNETPP_OUT_DIR must be correctly set
!endif
!if "$(OMNETPP_LIB_DIR)"==""
!error OMNETPP_LIB_DIR must be correctly set
!endif

#
# Local definitions
#
MAINLIBNAME=oppmain$D
LIBNAME=oppenvir$D
O=$(OMNETPP_OUT_DIR)/$(OUTPUT_PREFIX)$(TOOLCHAIN_NAME)-$(MODE)/src/envir
L=$(OMNETPP_LIB_DIR)/$(OUTPUT_PREFIX)$(TOOLCHAIN_NAME)

COPTS= $(CFLAGS) -I"$(OMNETPP_INCL_DIR)" -I"$(OMNETPP_INCL_DIR)/platdep" -I../sim  -I../common -DHAVE_DLOPEN=$(HAVE_DLOPEN) -DSHARED_LIB_SUFFIX="\"$(SHARED_LIB_SUFFIX)\"" /Fd"$O/$(LIBNAME).pdb"
IMPLIBS= $O/../sim/oppsim$D.lib $O/../nedxml/oppnedxml$D.lib $O/../common/oppcommon$D.lib $(XML_LIBS)

OBJS= $O/appreg.obj $O/args.obj $O/startup.obj $O/evmain.obj $O/logformatter.obj $O/envirbase.obj $O/fsutils.obj \
      $O/sectionbasedconfig.obj $O/inifilereader.obj $O/scenario.obj $O/valueiterator.obj \
      $O/fileoutvectormgr.obj $O/fileoutscalarmgr.obj $O/filesnapshotmgr.obj \
      $O/indexedfileoutvectormgr.obj $O/akoutvectormgr.obj \
      $O/speedometer.obj $O/matchableobject.obj $O/matchablefield.obj \
      $O/akaroarng.obj $O/cxmldoccache.obj $O/eventlogwriter.obj $O/objectprinter.obj \
      $O/eventlogfilemgr.obj $O/runattributes.obj $O/intervals.obj \
      $O/statisticparser.obj $O/visitor.obj

#TODO make configurable; put into separate lib? (src/httpserver)
OBJS= $(OBJS) $O/httpserver.obj $O/mongoosehttpserver.obj $O/mongoose.obj $O/json.obj

GENERATED_SOURCES= eventlogwriter.cc eventlogwriter.h

!if "$(LIB_SUFFIX)"==".dll"
COPTS= $(COPTS) -DENVIR_EXPORT
!endif

!if "$(WITH_NETBUILDER)"=="yes"
COPTS=$(COPTS) -DWITH_NETBUILDER
!endif

#
# Automatic rules
#
.SUFFIXES : .cc

{.}.cc{$O}.obj:
	$(Q)$(CXX) /Fo"$@" $(CXXFLAGS) $(COPTS) /c /Tp $<

#
# Targets
#
all : $O/$(LIBNAME)$(LIB_SUFFIX)

$O/$(LIBNAME).lib : $(OBJS) $L/$(MAINLIBNAME).lib $L
	@echo Creating LIB: $@
	$(Q)$(AR) /out:"$@" $(OBJS)
	$(Q)copy $(O:/=\)\$(LIBNAME).lib $(L:/=\) 1>nul
	-$(Q)copy /b $(O:/=\)\$(LIBNAME).pdb $(L:/=\) 1>nul

envir-implib : $(OBJS) $L
	@echo Creating envir/implib...
	@rem How can one create only the import lib, without trying to link the DLL?
        @rem With this command, we need the '>nul' to hide the 'undefined symbol' errors
	-$(Q)$(SHLIB_LD) /out:$(O:/=\)\$(LIBNAME).dll /subsystem:console $(OBJS) $(LDFLAGS) >nul 2>nul

$O/$(LIBNAME).dll : $(OBJS) $L/$(MAINLIBNAME).lib $L
	@echo Creating DLL: $@
	$(Q)$(SHLIB_LD) /out:"$@" /subsystem:console $(OBJS) $(IMPLIBS) $(LDFLAGS)
	$(Q)copy $(O:/=\)\$(LIBNAME).dll $(OMNETPP_BIN_DIR:/=\) 1>nul
	-$(Q)copy /b $(O:/=\)\$(LIBNAME).dll.manifest $(OMNETPP_BIN_DIR:/=\) 1>nul
	$(Q)copy $(O:/=\)\$(LIBNAME).lib $(L:/=\) 1>nul
	-$(Q)copy /b $(O:/=\)\$(LIBNAME).pdb $(L:/=\) 1>nul

$L/$(MAINLIBNAME).lib : $O/main.obj
	@echo Creating LIB: $@
	$(Q)$(AR) /out:"$(O:/=\)\$(MAINLIBNAME).lib" $O/main.obj
	$(Q)copy $(O:/=\)\$(MAINLIBNAME).lib $(L:/=\) 1>nul
	-$(Q)copy /b $(O:/=\)\$(MAINLIBNAME).pdb $(L:/=\) 1>nul

eventlogwriter.cc eventlogwriter.h : eventlogwriter.pl ../eventlog/eventlogentries.txt
	$(qecho) Running eventlogwriter.pl
	$(Q)$(PERL) eventlogwriter.pl

$O/envirbase.obj: eventlogwriter.h

$O/mongoose.obj : mongoose.c mongoose.h
	$(CXX) /Fo"$@" $(COPTS) /DHAVE_STRTOUI64 /c /TC mongoose.c

# default opp_run target - called only at the end once tkenv and cmdenv is done
# opp_run is useless with static libs
!if "$(LIB_SUFFIX)"==".dll"
!if "$(MODE)"=="debug"
opp_run_executable: $O/opp_run.exe

$O/opp_run.exe : $O/opp_run.obj $O/$(LIBNAME)$(LIB_SUFFIX)
	@echo Creating EXE: $@
	$(Q)$(LINK) $(LDFLAGS) /nologo /out:"$@" $O/opp_run.obj /libpath:$L $(ALL_ENV_LIBS) $(KERNEL_LIBS) $(SYS_LIBS) psapi.lib
	$(Q)copy $(O:/=\)\opp_run.exe $(OMNETPP_BIN_DIR:/=\) 1>nul
	-$(Q)copy $(O:/=\)\opp_run.exe.manifest $(OMNETPP_BIN_DIR:/=\) 1>nul
!else
opp_run_executable: $O/opp_run_release.exe

$O/opp_run_release.exe : $O/main.obj $O/$(LIBNAME)$(LIB_SUFFIX)
	@echo Creating EXE: $@
	$(Q)$(LINK) $(LDFLAGS) /nologo /out:"$@" $O/main.obj /libpath:$L $(ALL_ENV_LIBS) $(KERNEL_LIBS) $(SYS_LIBS)
	$(Q)copy $(O:/=\)\opp_run_release.exe $(OMNETPP_BIN_DIR:/=\) 1>nul
	-$(Q)copy $(O:/=\)\opp_run_release.exe.manifest $(OMNETPP_BIN_DIR:/=\) 1>nul
	$(Q)copy $(O:/=\)\opp_run_release.exe $(OMNETPP_BIN_DIR:/=\)\opp_run.exe 1>nul
	-$(Q)copy $(O:/=\)\opp_run_release.exe.manifest $(OMNETPP_BIN_DIR:/=\)\opp_run.exe.manifest 1>nul
!endif

$O/opp_run.obj : opp_run.cc $O
	@: Note: not in OBJS because opp_run.cc must be compiled *without* -DENVIR_EXPORT
	$(Q)$(CXX) $(CFLAGS) -I"$(OMNETPP_INCL_DIR)" -I"$(OMNETPP_INCL_DIR)/platdep" -I../sim  -I../common -DHAVE_DLOPEN=$(HAVE_DLOPEN) /c /Fo"$@" /Tp opp_run.cc

$O/main.obj : main.cc $O
	@: Note: not in OBJS because main.cc must be compiled *without* -DENVIR_EXPORT
	$(Q)$(CXX) $(CFLAGS) -I"$(OMNETPP_INCL_DIR)" -I"$(OMNETPP_INCL_DIR)/platdep" -I../sim  -I../common -DHAVE_DLOPEN=$(HAVE_DLOPEN) /c /Fo"$@" /Tp main.cc

!else
opp_run_executable:
	echo Note: We don't create opp_run.exe when building static libs
!endif

$(OBJS): $O

$O $L:
	@md $(@:/=\)

depend: $(GENERATED_SOURCES)
	$(qecho) Creating dependencies...
	$(Q)$(MAKEDEPEND) -I../../include -fMakefile.vc -P^$O/ -- *.cc

clean:
	$(qecho) Cleaning...
	-$(Q)del -f $(GENERATED_SOURCES) 2>nul
	-$(Q)rmdir /s /q $(O:/=\) 2>nul
	-$(Q)del /q $(L:/=\)\$(LIBNAME).* $(OMNETPP_LIB_DIR:/=\)\$(LIBNAME).* 2>nul
	-$(Q)cd $(OMNETPP_BIN_DIR:/=\) && del /q opp_run.exe* 2>nul

