#
# Global definitions
#
!include ..\..\configuser.vc

!if "$(LIB_SUFFIX)"==".dll"
CFLAGS=$(CFLAGS) -DWIN32_DLL
!endif

!if "$(WITH_NETBUILDER)"=="yes"
CFLAGS=$(CFLAGS) -DWITH_NETBUILDER
!endif

#
# Local definitions
#
COPTS=$(CFLAGS) -I"$(OMNETPP_INCL_DIR)" -I"$(OMNETPP_INCL_DIR)/platdep" -I../sim  -I../common -DBUILDING_ENVIR -DHAVE_DLOPEN=$(HAVE_DLOPEN) /Fd"envir.pdb"
IMPLIBS=../sim/sim_std.lib ../nedxml/nedxml.lib ../common/common.lib

OBJS=args.obj cenvir.obj cinifile.obj main.obj omnetapp.obj fsutils.obj \
     speedmtr.obj filemgrs.obj akoutvectormgr.obj matchableobject.obj \
     akaroarng.obj cxmldoccache.obj ivfilemgr.obj eventlogwriter.obj


#
# Automatic rules
#
.SUFFIXES : .cc

.cc.obj:
	$(CXX) /Fo"$@" $(COPTS) /c /Tp $<

#
# Targets
#
all : envir$(LIB_SUFFIX)

envir.lib : $(OBJS)
	$(AR) /out:envir.lib $(OBJS)
	copy envir.lib $(OMNETPP_LIB_DIR:/=\)
	- copy /b envir.pdb $(OMNETPP_LIB_DIR:/=\) 2>nul

envir-implib : $(OBJS)
	@rem How can one create only the import lib, without trying to link the DLL?
        @rem With this command, we need the '>nul' to hide the 'undefined symbol' errors
	- $(SHLIB_LD) /out:envir.dll /subsystem:console $(OBJS) >nul

envir.dll : $(OBJS)
	$(SHLIB_LD) /out:envir.dll /subsystem:console $(OBJS) $(IMPLIBS)
	copy envir.dll $(OMNETPP_BIN_DIR:/=\)
	copy envir.lib $(OMNETPP_LIB_DIR:/=\)

eventlogwriter.cc eventlogwriter.h : eventlogentries.pl
	$(PERL) eventlogentries.pl

omnetapp.o: eventlogwriter.h

depend:
	$(MAKEDEPEND) -I../../include -fMakefile.vc -- *.cc

clean:
	-del *.obj envir.lib envir.dll envir.lib envir.exp 2> nul
	-del $(VC_AUX_FILES) 2> nul

