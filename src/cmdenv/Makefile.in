#
# Definitions to be customized by 'configure'
#
CXX=@CXX@
AR=@AR@
SHLIB_LD=@SHLIB_LD@
RANLIB=@RANLIB@
MAKEDEPEND=@MAKEDEPEND@

CFLAGS=@CFLAGS@

OMNETPP_INCL_DIR=@OMNETPP_INCL_DIR@
OMNETPP_LIB_DIR=@OMNETPP_LIB_DIR@
OMNETPP_VERSION=@OMNETPP_VERSION@

# LIB_SUFFIX is either A_LIB_SUFFIX or SO_LIB_SUFFIX
LIB_SUFFIX=@LIB_SUFFIX@

A_LIB_SUFFIX=@A_LIB_SUFFIX@
SO_LIB_SUFFIX=@SO_LIB_SUFFIX@

#
# Local definitions
#
COPTS=$(CFLAGS) -I$(OMNETPP_INCL_DIR) -I../envir -DBUILDING_CMDENV

OBJS = cmdenv.o enumstr.o heap.o

#
# Targets
#
all: libcmdenv$(LIB_SUFFIX)

libcmdenv$(A_LIB_SUFFIX) : $(OBJS)
	$(AR) libcmdenv$(A_LIB_SUFFIX) $(OBJS)
	$(RANLIB) libcmdenv$(A_LIB_SUFFIX)
	../utils/install-lib libcmdenv$(A_LIB_SUFFIX).$(OMNETPP_VERSION) $(OMNETPP_LIB_DIR) libcmdenv$(A_LIB_SUFFIX)

libcmdenv$(SO_LIB_SUFFIX) : $(OBJS)
	$(SHLIB_LD) -o libcmdenv$(SO_LIB_SUFFIX) $(OBJS)
	../utils/install-lib libcmdenv$(SO_LIB_SUFFIX).$(OMNETPP_VERSION) $(OMNETPP_LIB_DIR) libcmdenv$(SO_LIB_SUFFIX)

cmdenv.o : cmdenv.cc
	$(CXX) -c $(COPTS) cmdenv.cc

enumstr.o : enumstr.cc
	$(CXX) -c $(COPTS) enumstr.cc

heap.o : heap.cc
	$(CXX) -c $(COPTS) heap.cc

depend:
	$(MAKEDEPEND) -I. -I../envir -I../../include -- *.cc
	$(MAKEDEPEND) -I. -I../envir -I../../include -fMakefile.in -- *.cc

clean:
	rm -f *.o libcmdenv$(A_LIB_SUFFIX) libcmdenv$(SO_LIB_SUFFIX)

# DO NOT DELETE THIS LINE -- make depend depends on it.
cmdenv.o: cmdenv.cc \
  cmddefs.h \
  cmdenv.h \
  enumstr.h \
  ../envir/appreg.h \
  ../envir/cinifile.h \
  ../../include/cmodule.h \
  ../../include/cnetmod.h \
  ../envir/args.h \
  ../../include/defs.h \
  ../../include/ctypes.h \
  ../../include/csimul.h \
  ../envir/omnetapp.h \
  ../../include/chead.h \
  ../../include/cobject.h \
  ../../include/util.h \
  ../../include/errmsg.h \
  ../../include/cmsgheap.h \
  ../../include/ccoroutine.h \
  ../../include/coutvect.h \
  ../../include/carray.h \
  ../../include/cenvir.h \
  ../envir/envdefs.h \
  ../../include/envirext.h \
  ../../include/cpar.h \
  ../../include/cqueue.h \
  ../../include/cgate.h
enumstr.o: enumstr.cc \
  enumstr.h
heap.o: heap.cc
