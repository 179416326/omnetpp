#
# Definitions to be customized by 'configure'
#
CXX=@CXX@
AS=@AS@
AR=@AR@
RANLIB=@RANLIB@
SHLIB_LD=@SHLIB_LD@
MAKEDEPEND=@MAKEDEPEND@

CFLAGS=@CFLAGS@

OMNETPP_INCL_DIR=@OMNETPP_INCL_DIR@
OMNETPP_LIB_DIR=@OMNETPP_LIB_DIR@
OMNETPP_VERSION=@OMNETPP_VERSION@

HAVE_MPI=@HAVE_MPI@
MPI_CFLAGS=@MPI_CFLAGS@

WITH_NETBUILDER=@WITH_NETBUILDER@
WITH_PARSIM=@WITH_PARSIM@

# LIB_SUFFIX is either A_LIB_SUFFIX or SO_LIB_SUFFIX
LIB_SUFFIX=@LIB_SUFFIX@

A_LIB_SUFFIX=@A_LIB_SUFFIX@
SO_LIB_SUFFIX=@SO_LIB_SUFFIX@

#
# Local definitions
#
COPTS=$(CFLAGS) -I$(OMNETPP_INCL_DIR) -I. -DBUILDING_SIM $(MPI_CFLAGS) $(PVM_CFLAGS)

CPPOBJS_STD = task.o ccoroutine.o ctypes.o carray.o cgate.o \
         cmodule.o csimplemodule.o csimul.o cobject.o cpar.o \
         cstat.o chist.o cdensity.o cvarhist.o cpsquare.o cksplit.o \
         cmessage.o cdetect.o util.o cllist.o ctopo.o errmsg.o \
         cmsgheap.o coutvect.o cqueue.o globals.o cwatch.o \
         cpacket.o cfsm.o cstruct.o cchannel.o cenum.o cexception.o cdispstr.o \
         cdefaultlist.o cscheduler.o onstartup.o random.o distrib.o
CPPOBJS_NETBUILDER= netbuilder/cdynamicmodule.o netbuilder/cdynamicnetwork.o \
                    netbuilder/cnednetworkbuilder.o netbuilder/loadnedfile.o
CPPOBJS_PARSIM = parsim/ccommbuffer.o parsim/cmemcommbuffer.o \
         parsim/cparsimsegment.o parsim/cplaceholdermod.o parsim/cproxygate.o \
         parsim/cparsimprotocolbase.o parsim/cnosynchronization.o \
         parsim/cnullmessageprot.o parsim/clinkdelaylookahead.o \
         parsim/cidealsimulationprot.o parsim/cispeventlogger.o \
         parsim/cparsimcomm.o \
         parsim/ccommbufferbase.o parsim/cfilecomm.o parsim/cfilecommbuffer.o \
         parsim/cnamedpipecomm.o parsim/creceivedexception.o
CPPOBJS_MPI = parsim/cmpicomm.o parsim/cmpicommbuffer.o

CPPOBJS= $(CPPOBJS_STD)

ifeq ($(WITH_NETBUILDER),yes)
CPPOBJS += $(CPPOBJS_NETBUILDER)
COPTS += /DWITH_NETBUILDER /I "$(OMNETPP_SRC_DIR)/nedxml"
endif

ifeq ($(WITH_PARSIM),yes)
CPPOBJS += $(CPPOBJS_PARSIM)
ifeq ($(HAVE_MPI),yes)
CPPOBJS += $(CPPOBJS_MPI)
COPTS += $(MPI_CFLAGS)
endif
endif

#
# Automatic rules
#
.SUFFIXES : .cc

.cc.o:
	$(CXX) -c $(COPTS) -o $@ $<

#
# Targets
#
default : sim_std

sim_std : libsim_std$(LIB_SUFFIX)

libsim_std$(A_LIB_SUFFIX) : $(CPPOBJS)
	$(AR) libsim_std$(A_LIB_SUFFIX) $(CPPOBJS)
	$(RANLIB) libsim_std$(A_LIB_SUFFIX)
	../utils/install-lib libsim_std$(A_LIB_SUFFIX).$(OMNETPP_VERSION) $(OMNETPP_LIB_DIR) libsim_std$(A_LIB_SUFFIX)

libsim_std$(SO_LIB_SUFFIX) : $(CPPOBJS)
	$(SHLIB_LD) -o libsim_std$(SO_LIB_SUFFIX) $(CPPOBJS)
	../utils/install-lib libsim_std$(SO_LIB_SUFFIX).$(OMNETPP_VERSION) $(OMNETPP_LIB_DIR) libsim_std$(SO_LIB_SUFFIX)

depend:
	$(MAKEDEPEND) -I../../include -- *.cc parsim/*.cc
	$(MAKEDEPEND) -I../../include -fMakefile.in -- *.cc parsim/*.cc

clean:
	rm -f *.o parsim/*.o
	rm -f libsim_std$(A_LIB_SUFFIX) libsim_std$(SO_LIB_SUFFIX)

