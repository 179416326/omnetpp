#
# variables set by `configure':
#
CXX=@CXX@
CC=@CC@

CFLAGS=@CFLAGS@
LDFLAGS=@LDFLAGS@

COPTS=$(CFLAGS)

OMNETPP_BIN_DIR=@OMNETPP_BIN_DIR@

seedtool : seedtool.o
	$(CC) $(LDFLAGS) seedtool.o -o seedtool

seedtool.o : seedtool.cc
	$(CXX) -c $(COPTS) seedtool.cc

depend:
	makedepend -Y -I. -- *.cc
	makedepend -Y -I. -fMakefile.in -- *.cc

clean:
	rm -f *.o seedtool

install-local: seedtool
	cp seedtool $(OMNETPP_BIN_DIR)
	(dir=`pwd`;cd $(OMNETPP_BIN_DIR);rm -f makemake;ln -s $$dir/makemake .)
	(dir=`pwd`;cd $(OMNETPP_BIN_DIR);rm -f splitvec;ln -s $$dir/splitvec .)

