#
# variables set by `configure':
#
CXX=@CXX@
LD=@LD@

CFLAGS=@CFLAGS@

COPTS=$(CFLAGS)

seedtool : seedtool.o
	$(LD) seedtool.o -o seedtool

seedtool.o : seedtool.cc
	$(CXX) -c $(COPTS) seedtool.cc

depend:
	makedepend -Y -I. -- *.cc
	makedepend -Y -I. -fmakefile.in -- *.cc

clean:
	rm -f *.o seedtool

# DO NOT DELETE THIS LINE -- make depend depends on it.


