ssm_ifs_n.cc: ssm_ifs.ned
	$(NEDC) $(INCLUDE_PATH) ssm_ifs.ned

fddi_mac.o: fddi_mac.cc
	$(CXX) -c $(COPTS) fddi_mac.cc

othermod.o: othermod.cc
	$(CXX) -c $(COPTS) othermod.cc

ssm_ifs.o: ssm_ifs.cc
	$(CXX) -c $(COPTS) ssm_ifs.cc


