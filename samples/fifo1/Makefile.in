#
#  Makefile for fifo1
#
#  ** This file was automatically generated by the command:
#  makemake -f -m
#

# Name of target to be created (-o option)
TARGET = fifo1

# User interface (uncomment one) (-u option)
# USERIF_LIBS=$(CMDENV_LIBS)
USERIF_LIBS=$(TKENV_LIBS)
# USERIF_LIBS=$(TVENV_LIBS)

# Uncomment the second line for parallel execution (PVM3) support (-p option):
KERNEL_LIBS=$(STD_KERNEL_LIBS)
# KERNEL_LIBS=$(PVM_KERNEL_LIBS)

# .ned or .h include paths with -I
INCLUDE_PATH=

# misc additional object and library files to link
EXRA_OBJS=

# object files in subdirectories
SUBDIR_OBJS=

# Additional libraries (-l option)
LIBS=

#------------------------------------------------------------------------------

NEDC=@NEDC@
CXX=@CXX@
CC=@CC@
AR=@AR@
SHLIB_LD=@SHLIB_LD@

CFLAGS=@CFLAGS@
LDFLAGS=@LDFLAGS@

OMNETPP_INCL_DIR=@OMNETPP_INCL_DIR@
OMNETPP_LIB_DIR=@OMNETPP_LIB_DIR@

TV_LIBS=@TV_LIBS@
TK_LIBS=@TK_LIBS@
PVM_LIBS=@PVM_LIBS@
SYS_LIBS=@SYS_LIBS@

# User interface libs
CMDENV_LIBS=-lenvir -lcmdenv
TVENV_LIBS=-lenvir -ltvenv $(TV_LIBS)
TKENV_LIBS=-lenvir -ltkenv $(TK_LIBS)

# Simulation kernel
STD_KERNEL_LIBS=-lsim_std
PVM_KERNEL_LIBS=-lsim_pvm $(PVM_LIBS)

# Simulation kernel and user interface libraries
OMNETPP_LIBS=-L$(OMNETPP_LIB_DIR) $(KERNEL_LIBS) $(USERIF_LIBS) $(SYS_LIBS)

COPTS=$(CFLAGS) $(INCLUDE_PATH) -I$(OMNETPP_INCL_DIR)

#------------------------------------------------------------------------------

# Object files from this directory to link
OBJS= fifo_n.o fifonet_n.o gen_n.o sink_n.o fifo.o gen.o sink.o

$(TARGET): $(OBJS) $(EXRA_OBJS) Makefile
	$(CC) $(LDFLAGS) $(OBJS) $(EXRA_OBJS) $(SUBDIR_OBJS) $(LIBS) $(OMNETPP_LIBS) -o $(TARGET)

fifo_n.o: fifo_n.cc
	$(CXX) -c $(COPTS) fifo_n.cc

fifo_n.cc: fifo.ned
	$(NEDC) $(INCLUDE_PATH) fifo.ned

fifonet_n.o: fifonet_n.cc
	$(CXX) -c $(COPTS) fifonet_n.cc

fifonet_n.cc: fifonet.ned
	$(NEDC) $(INCLUDE_PATH) fifonet.ned

gen_n.o: gen_n.cc
	$(CXX) -c $(COPTS) gen_n.cc

gen_n.cc: gen.ned
	$(NEDC) $(INCLUDE_PATH) gen.ned

sink_n.o: sink_n.cc
	$(CXX) -c $(COPTS) sink_n.cc

sink_n.cc: sink.ned
	$(NEDC) $(INCLUDE_PATH) sink.ned

fifo.o: fifo.cc
	$(CXX) -c $(COPTS) fifo.cc

gen.o: gen.cc
	$(CXX) -c $(COPTS) gen.cc

sink.o: sink.cc
	$(CXX) -c $(COPTS) sink.cc

clean:
	rm -f *.o *_n.cc $(TARGET)
	rm -f *.vec *.sca

depend:
	makedepend -Y $(INCLUDE_PATH) -- *.cc
	makedepend -Y $(INCLUDE_PATH) -fMakefile.in -- *.cc

re-makemake:
	makemake -f    #recreate Makefile

re-makemake-m:
	makemake -f -m    #recreate Makefile.in

# DO NOT DELETE THIS LINE -- make depend depends on it.

fifo.o: fifo.h
gen.o: gen.h
sink.o: sink.h
