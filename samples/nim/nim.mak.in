!IF "$(CFG)" == ""
CFG=nim - Win32 Debug Tkenv
!MESSAGE No configuration specified. Defaulting to nim - Win32 Debug Tkenv.
!ENDIF

!IF "$(CFG)" != "nim - Win32 Debug Tkenv" && "$(CFG)" != "nim - Win32 Debug Cmdenv" && "$(CFG)" != "nim - Win32 Release Tkenv" && "$(CFG)" != "nim - Win32 Release Cmdenv"
