# Additional libraries (-l option)
LIBS=

#------------------------------------------------------------------------------

NEDC=@NEDC@
CXX=@CXX@
CC=@CC@
AR=@AR@
SHLIB_LD=@SHLIB_LD@

CFLAGS=@CFLAGS@
LDFLAGS=@LDFLAGS@
EXE_SUFFIX=@EXE_SUFFIX@

OMNETPP_INCL_DIR=@OMNETPP_INCL_DIR@
OMNETPP_LIB_DIR=@OMNETPP_LIB_DIR@

TV_LIBS=@TV_LIBS@
TK_LIBS=@TK_LIBS@
PVM_LIBS=@PVM_LIBS@
SYS_LIBS=@SYS_LIBS@

# User interface libs
CMDENV_LIBS=-lenvir -lcmdenv
TKENV_LIBS=-lenvir -ltkenv $(TK_LIBS)

# Simulation kernel
STD_KERNEL_LIBS=-lsim_std
PVM_KERNEL_LIBS=-lsim_pvm $(PVM_LIBS)

# Simulation kernel and user interface libraries
OMNETPP_LIBS=-L$(OMNETPP_LIB_DIR) $(USERIF_LIBS) $(KERNEL_LIBS) $(SYS_LIBS)

COPTS=$(CFLAGS) $(INCLUDE_PATH) -I$(OMNETPP_INCL_DIR)

#------------------------------------------------------------------------------

# Object files from this directory to link
OBJS=

$(TARGET): $(OBJS) $(EXRA_OBJS) Makefile subdirs-phony
	$(CC) $(LDFLAGS) $(OBJS) $(EXRA_OBJS) $(SUBDIR_OBJS) $(LIBS) $(OMNETPP_LIBS) -o $(TARGET)

subdirs-phony:
	(cd ../dyna && make)
	(cd ../fddi && make)
	(cd ../fifo1 && make)
	(cd ../fifo2 && make)
	(cd ../hcube && make)
	(cd ../hist && make)
	(cd ../nim && make)
	(cd ../token && make)
	(cd ../topo && make)

clean:
	rm -f *.o *_n.cc $(TARGET)$(EXE_SUFFIX)
	rm -f *.vec *.sca

depend:


re-makemake:
	opp_makemake -f  ../dyna ../fddi ../fifo1 ../fifo2 ../hcube ../hist ../nim ../token ../topo  #recreate Makefile

re-makemake-m:
	opp_makemake -f -m  ../dyna ../fddi ../fifo1 ../fifo2 ../hcube ../hist ../nim ../token ../topo  #recreate Makefile.in

