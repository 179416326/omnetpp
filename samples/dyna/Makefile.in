#
#  Makefile for dyna
#
#  ** This file was automatically generated by the command:
#  opp_makemake -f -m
#

# Name of target to be created (-o option)
TARGET = dyna

# User interface (uncomment one) (-u option)
# USERIF_LIBS=$(CMDENV_LIBS)
USERIF_LIBS=$(TKENV_LIBS)

# Uncomment the second line for parallel execution (PVM3) support (-p option):
KERNEL_LIBS=$(STD_KERNEL_LIBS)
# KERNEL_LIBS=$(PVM_KERNEL_LIBS)

# .ned or .h include paths with -I
INCLUDE_PATH=

# misc additional object and library files to link
EXRA_OBJS=

# object files in subdirectories
SUBDIR_OBJS=

# Additional libraries (-l option)
LIBS=

#------------------------------------------------------------------------------

NEDC=@NEDC@
CXX=@CXX@
CC=@CC@
AR=@AR@
SHLIB_LD=@SHLIB_LD@
MAKEDEPEND=@MAKEDEPEND@

CFLAGS=@CFLAGS@
LDFLAGS=@LDFLAGS@
EXE_SUFFIX=@EXE_SUFFIX@
DEPFLAGS=@DEPFLAGS@

OMNETPP_INCL_DIR=@OMNETPP_INCL_DIR@
OMNETPP_LIB_DIR=@OMNETPP_LIB_DIR@

TK_LIBS=@TK_LIBS@
PVM_LIBS=@PVM_LIBS@
SYS_LIBS=@SYS_LIBS@
SYS_LIBS_PURE=-lm $(shell $(CXX) -print-file-name=libstdc++.a)

# User interface libs
CMDENV_LIBS=-lenvir -lcmdenv
TKENV_LIBS=-lenvir -ltkenv $(TK_LIBS)

# Simulation kernel
STD_KERNEL_LIBS=-lsim_std
PVM_KERNEL_LIBS=-lsim_pvm $(PVM_LIBS)

# Simulation kernel and user interface libraries
OMNETPP_LIBS=-L$(OMNETPP_LIB_DIR) $(KERNEL_LIBS) $(USERIF_LIBS) $(SYS_LIBS)

COPTS=$(CFLAGS) $(INCLUDE_PATH) -I$(OMNETPP_INCL_DIR)

#------------------------------------------------------------------------------

# Object files from this directory to link
OBJS= dyna_n.o client.o server.o servproc.o switch.o

$(TARGET): $(OBJS) $(EXRA_OBJS) Makefile
	$(CC) $(LDFLAGS) $(OBJS) $(EXRA_OBJS) $(SUBDIR_OBJS) $(LIBS) $(OMNETPP_LIBS) -o $(TARGET)

purify: $(OBJS) $(EXRA_OBJS)   Makefile
	purify $(CC) $(LDFLAGS) $(OBJS) $(EXRA_OBJS) $(SUBDIR_OBJS) $(LIBS) -L$(OMNETPP_LIB_DIR) $(KERNEL_LIBS) $(USERIF_LIBS) $(SYS_LIBS_PURE) -o $(TARGET).pure

dyna_n.o: dyna_n.cc
	$(CXX) -c $(COPTS) dyna_n.cc

dyna_n.cc: dyna.ned
	$(NEDC) $(INCLUDE_PATH) dyna.ned

client.o: client.cc
	$(CXX) -c $(COPTS) client.cc

server.o: server.cc
	$(CXX) -c $(COPTS) server.cc

servproc.o: servproc.cc
	$(CXX) -c $(COPTS) servproc.cc

switch.o: switch.cc
	$(CXX) -c $(COPTS) switch.cc

clean:
	rm -f *.o *_n.cc $(TARGET)$(EXE_SUFFIX)
	rm -f *.vec *.sca

depend:
	$(MAKEDEPEND) $(DEPFLAGS) $(INCLUDE_PATH) -- *.cc
	$(MAKEDEPEND) $(DEPFLAGS) $(INCLUDE_PATH) -fMakefile.in -- *.cc

re-makemake:
	opp_makemake -f    #recreate Makefile

re-makemake-m:
	opp_makemake -f -m    #recreate Makefile.in

# DO NOT DELETE THIS LINE -- make depend depends on it.

