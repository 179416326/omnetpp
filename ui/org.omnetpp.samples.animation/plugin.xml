<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.omnetpp.samples.animation.editors.CustomAnimationEditor"
            contributorClass="org.omnetpp.animation.editors.AnimationContributor"
            default="false"
            extensions="anim"
            icon="icons/full/eobj16/animation.gif"
            id="org.omnetpp.samples.animation.editors.CustomAnimationEditor"
            name="Custom Animation Player">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="org.omnetpp.views"
            class="org.omnetpp.samples.animation.editors.CustomAnimationView"
            icon="icons/full/eview16/animation.gif"
            id="org.omnetpp.samples.animation.editors.CustomAnimationView"
            name="Custom Animation">
      </view>
   </extension>
</plugin>
