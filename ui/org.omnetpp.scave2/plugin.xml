<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         point="org.eclipse.ui.editors">
      <editor
            name="OMNeT++ Analysis"
            extensions="scave"
            icon="icons/sample.gif"
            contributorClass="org.omnetpp.scave.model.presentation.ScaveModelActionBarContributor"
            class="org.omnetpp.scave2.editors.ScaveEditor"
            id="org.omnetpp.scave2.editors.ScaveEditor">
      </editor>
   </extension>
   <extension
         id="org.omnetpp.scave.content-types"
         name="Scave content types"
         point="org.eclipse.core.runtime.contentTypes">
      <content-type
            file-extensions="sca"
            id="org.omnetpp.scave.content-type.scalar"
            name="Scalar result file"
            priority="normal"/>
      <content-type
            file-extensions="vec"
            id="org.omnetpp.scave.content-type.vector"
            name="Vector result file"
            priority="normal"/>
   </extension>

</plugin>
