<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.omnetpp.animation.editors.EventLogAnimationEditor"
            contributorClass="org.omnetpp.animation.editors.EventLogAnimationContributor"
            default="false"
            extensions="elog"
            icon="icons/full/eobj16/animation.gif"
            id="org.omnetpp.animation.editors.EventLogAnimationEditor"
            name="OMNeT++ Animation Player (Technology Preview)">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="org.omnetpp.views"
            class="org.omnetpp.animation.editors.EventLogAnimationView"
            icon="icons/full/eview16/animation.gif"
            id="org.omnetpp.animation.editors.EventLogAnimationView"
            name="Animation (Technology Preview)">
      </view>
   </extension>
</plugin>
