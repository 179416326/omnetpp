<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>

   <extension
         point="org.eclipse.ui.editors">
      <editor
            name="Scalar Dataset Editor"
            extensions="sdataset"
            icon="icons/sdataset.gif"
            contributorClass="org.omnetpp.scave.editors.ScalarDatasetEditorContributor"
            class="org.omnetpp.scave.editors.ScalarDatasetEditor"
            id="org.omnetpp.scave.editors.ScalarDatasetEditor">
      </editor>
      <editor
            name="Vector Dataset Editor"
            extensions="vdataset"
            icon="icons/vdataset.gif"
            contributorClass="org.omnetpp.scave.editors.VectorDatasetEditorContributor"
            class="org.omnetpp.scave.editors.VectorDatasetEditor"
            id="org.omnetpp.scave.editors.VectorDatasetEditor">
      </editor>
      <editor
            name="Scalar Chart Editor"
            extensions="schart"
            icon="icons/barchart.gif"
            contributorClass="org.omnetpp.scave.editors.ScalarChartEditorContributor"
            class="org.omnetpp.scave.editors.ScalarChartEditor"
            id="org.omnetpp.scave.editors.ScalarChartEditor">
      </editor>
      <editor
            name="Vector Chart Editor"
            extensions="vchart"
            icon="icons/xyplot.gif"
            contributorClass="org.omnetpp.scave.editors.VectorChartEditorContributor"
            class="org.omnetpp.scave.editors.VectorChartEditor"
            id="org.omnetpp.scave.editors.VectorChartEditor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            objectClass="org.eclipse.core.resources.IFile"
            nameFilter="plugin.xml"
            id="org.omnetpp.scave.contribution1">
         <menu
               label="New Submenu"
               path="additions"
               id="org.omnetpp.scave.menu1">
            <separator
                  name="group1">
            </separator>
         </menu>
         <action
               label="New Action"
               class="org.omnetpp.scave.popup.actions.NewAction"
               menubarPath="org.omnetpp.scave.menu1/group1"
               enablesFor="1"
               id="org.omnetpp.scave.newAction">
         </action>
      </objectContribution>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipse.ui.ide.workbench"
            name="Scave">
         <property
               name="aboutText"
               value="%productBlurb"/>
         <property
               name="aboutImage"
               value="eclipse_lg.gif"/>
      </product>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="org.omnetpp.scave.plugin.ScavePerspective"
            icon="icons/sample.gif"
            id="org.omnetpp.scave.ScavePerspective"
            name="Result Analysis - OMNeT++"/>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="org.omnetpp.scave.New"
            name="Result analysis - OMNeT++"/>
      <wizard
            category="org.omnetpp.scave.New"
            class="org.omnetpp.scave.wizards.SampleNewWizard"
            icon="icons/sample.gif"
            id="org.omnetpp.scave.wizards.SampleNewWizard"
            name="Sample New Wizard"/>
      <wizard
            canFinishEarly="true"
            category="org.omnetpp.scave.New"
            class="org.omnetpp.scave.wizards.UntitledTextFileWizard"
            hasPages="false"
            icon="icons/sample.gif"
            id="org.omnetpp.scave.wizards.SampleNewWizard2"
            name="Whatever new scave wizard"/>
   </extension>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            id="org.omnetpp.scave.ActionSet"
            label="Scava ActionSet 1"
            visible="true">
         <action
               class="org.omnetpp.scave.actions.NewScalarDatasetActionDelegate"
               icon="icons/sdataset.gif"
               id="org.omnetpp.scave.NewScalarDatasetAction"
               label="New Scalar Dataset"
               style="push"
               toolbarPath="scave"
               tooltip="New Scalar Dataset"/>
         <action
               class="org.omnetpp.scave.actions.NewVectorDatasetActionDelegate"
               icon="icons/vdataset.gif"
               id="org.omnetpp.scave.NewVectorDatasetAction"
               label="New Vector Dataset"
               style="push"
               toolbarPath="scave"
               tooltip="New Vector Dataset"/>
         <action
               class="org.omnetpp.scave.actions.NewScalarChartActionDelegate"
               icon="icons/barchart.gif"
               id="org.omnetpp.scave.NewScalarChartAction"
               label="New Scalar Chart"
               style="push"
               toolbarPath="scave"
               tooltip="New Scalar Chart"/>
         <action
               class="org.omnetpp.scave.actions.NewVectorChartActionDelegate"
               icon="icons/xyplot.gif"
               id="org.omnetpp.scave.NewVectorChartAction"
               label="New Vector Chart"
               style="push"
               toolbarPath="scave"
               tooltip="New Vector Chart"/>
      </actionSet>
   </extension>
   <extension
         point="org.eclipse.ui.editorActions">
   </extension>

</plugin>
