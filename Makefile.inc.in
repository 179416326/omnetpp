DLLTOOL = @DLLTOOL@
WISH = @WISH@
MAKEDEPEND = @MAKEDEPEND@
DOT = @DOT@
DOXYGEN = @DOXYGEN@

A_LIB_SUFFIX = @A_LIB_SUFFIX@
SO_LIB_SUFFIX = @SO_LIB_SUFFIX@
DLL_LIB_SUFFIX = @DLL_LIB_SUFFIX@
EXE_SUFFIX = @EXE_SUFFIX@
