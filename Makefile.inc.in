SWIG = @SWIG@
ECLIPSE_PLATFORM = @ECLIPSE_PLATFORM@

#
# Definitions for makemake-generated makefiles
#
ALL_ENV_LIBS = @ALL_ENV_LIBS@
CMDENV_LIBS = -u_cmdenv_lib -lenvir$D -lcmdenv$D
TKENV_LIBS = -u_tkenv_lib -lenvir$D -ltkenv$D -llayout$D $(TK_LIBS) $(ZLIB_LIBS)
KERNEL_LIBS = -lsim_std$D

ifeq ($(WITH_NETBUILDER),yes)
KERNEL_LIBS += -lnedxml$D $(XML_LIBS)
endif

KERNEL_LIBS += -lcommon$D

ifeq ($(WITH_PARSIM),yes)
KERNEL_LIBS += $(MPI_LIBS)
endif

ifeq ("$(BUILD_SHARED_LIBS)", "yes")
LIB_SUFFIX =  $(SHARED_LIB_SUFFIX)
else
LIB_SUFFIX = $(A_LIB_SUFFIX)
endif

SYS_LIBS = @SYS_LIBS@

