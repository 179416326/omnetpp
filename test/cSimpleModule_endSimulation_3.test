%description:
perverse case: endSimulation() from initialize()

%module: {Module}

class {Module} : public cSimpleModule
{
    Module_Class_Members({Module},cSimpleModule,0);
    virtual void initialize();
    virtual void handleMessage(cMessage *msg);
    virtual void finish();
};

Define_Module({Module});

void {Module}::initialize()
{
    scheduleAt(1.0, new cMessage("msg"));
    endSimulation();
    ev << "after endSimulation\n";
}

void {Module}::handleMessage(cMessage *msg)
{
    scheduleAt(simTime()+1.0, msg);
    ev << "event after endSimulation\n";
}

void {Module}::finish()
{
    ev << "finish called\n";
}

%not-contains: stdout
after endSimulation

%not-contains: stdout
event after endSimulation

