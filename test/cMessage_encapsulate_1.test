%name: cMessage_encapsulate_1

%description:
Tests cMessage.encapsulate().

%source:
cMessage *payload = new cMessage;
payload->setLength(128);

cMessage *frame = new cMessage;
frame->setLength(32);

frame->encapsulate(payload);
ev << frame->length() << endl;

frame->decapsulate();
ev << frame->length() << endl;

ev << payload->length() << endl;

%ignore-exitcode: 1

%contains: stdout
160
32
128

