%description:
**EXPERIMENTAL TEST**
Test oparators and precedence in NED.

%file: {}.ned

simple {Simple}
   parameters:
      p00, p01, p02, p03, p04, p05, p06, p07, p08, p09,
      p10, p11, p12, p13, p14, p15, p16, p17, p18, p19,
      p20, p21, p22, p23, p24, p25, p26, p27, p28, p29,
      p30, p31, p32, p33, p34, p35, p36, p37, p38, p39,
      p40, p41, p42, p43, p44, p45, p46, p47;
endsimple

network {net}: {Simple}
   parameters:
      p00 = -3,      // unary minus
      p01 = -(4+1),  // unary minus

      p02 = ! true,  // negation
      p03 = ! false, // negation
      p04 = ! 0,     // negation
      p05 = ! 3,     // negation

      p06 = ~3,      // bitwise complement
      p07 = 12 ^ 3,  // power of
      p08 = 12 * 3,  // multiply
      p09 = 12 / 3,  // divide
      p10 = 12 % 3,  // modulus
      p11 = 12 + 3,  // add
      p12 = 12 - 3,  // subtract

      p13 = 10 << 2, // bitwise shifting
      p14 = 10 >> 2, // bitwise shifting

      p15 = 10 & 3,  // bitwise and
      p16 = 10 | 3,  // bitwise or
      p17 = 10 # 3,  // bitwise xor

      p18 = 8 == 3, // equal
      p19 = 8 == 8, // equal

      p20 = 8 != 3, // not equal
      p21 = 8 != 8, // not equal

      p22 = 8 > 3,  // greater
      p23 = 3 > 8,  // greater
      p24 = 8 > 8,  // greater

      p25 = 8 >= 3, // greater or equal
      p26 = 3 >= 8, // greater or equal
      p27 = 8 >= 8, // greater or equal

      p28 = 8 < 3,  // less
      p29 = 3 < 8,  // less
      p30 = 8 < 8,  // less

      p31 = 8 <= 3, // less or equal
      p32 = 3 <= 8, // less or equal
      p33 = 8 <= 8, // less or equal

      p34 = 8 && 3, // logical and
      p35 = 8 && 0, // logical and
      p36 = 0 && 3, // logical and
      p37 = 0 && 0, // logical and

      p38 = 8 || 3, // logical or
      p39 = 8 || 0, // logical or
      p40 = 0 || 3, // logical or
      p41 = 0 || 0, // logical or

      p42 = 8 ## 3, // logical xor
      p43 = 8 ## 0, // logical xor
      p44 = 0 ## 3, // logical xor
      p45 = 0 ## 0, // logical xor

      p46 = true ? 5 : 2,  // the C/C++ ``inline if''
      p47 = false ? 5 : 2; // the C/C++ ``inline if''

endnetwork

%file: {}.cc

#include <omnetpp.h>

class {Simple} : public cSimpleModule
{
    Module_Class_Members({Simple},cSimpleModule,16384)
    virtual void activity();
};

Define_Module({Simple});

void {Simple}::activity()
{
    for (int i=0; i<params(); i++)
        ev << par(i).name() << "=" << par(i) << endl;
}

%inifile: {}.ini
[General]
network={net}

%contains: stdout

