%description:
check that "like" keyword works for modules (TODO check for channels too)

%file: test.ned

moduleinterface IFoo
{
}

simple Foo like IFoo
{
}

network Test
{
    parameters:
        string modType;
    submodules:
        specMod: <modType> like IFoo;
}

%file: test.cc

#include <omnetpp.h>

namespace {} {

class Foo : public cSimpleModule
{
  public:
    Foo() : cSimpleModule(16384) { }
    virtual void activity();
};

Define_Module(Foo);

void Foo::activity()
{
   ev << "Here!" << endl;
}

}; //namespace

%inifile: test.ini
[General]
network = Test
cmdenv-express-mode = false
*.modType = "Foo"

%contains-regex: stdout
Here!

