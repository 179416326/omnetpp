%description:
check that "like" keyword works

%file: test.ned

simple GenMod
{
}

network Test
{
    parameters:
        string mod_type;
    submodules:
        specMod: <mod_type> like GenMod;
}

%file: test.cc

#include <omnetpp.h>

namespace {} {

class GenMod : public cSimpleModule
{
  public:
    GenMod() : cSimpleModule(16384) { }
    virtual void activity();
};

Define_Module(GenMod);

void GenMod::activity()
{
   ev << "Here!" << endl;
}

}; //namespace

%inifile: test.ini
[General]
network = Test
warnings = yes
cmdenv-express-mode = false
*.mod_type = "GenMod"

%contains-regex: stdout
Here!

