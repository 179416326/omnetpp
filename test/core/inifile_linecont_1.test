%description:
check that backslash line continuation works

%file: test.ned

simple Simple
{
    parameters:
        string str;
}

network Net extends Simple
{
}

%file: test.cc

#include <omnetpp.h>

namespace {} {

class Simple : public cSimpleModule
{
  public:
    Simple() : cSimpleModule(16384) { }
    virtual void activity();
};

Define_Module(Simple);

void Simple::activity()
{
    ev << "\"" <<(const char *)par("str") << "\"" << endl;
    ev << "." << endl;
}

}; //namespace


%inifile: test.ini
[General]
network=Net

[Parameters]
*.str="one \
two  \
three\
    four"
%contains: stdout
"one two  three    four"
.





