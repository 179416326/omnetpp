%description:
check that 1023 long lines are accepted

%file: test.ned

simple Simple
{
    parameters:
        string verylongstring;
}

%file: test.cc

#include <string.h>
#include <omnetpp.h>

namespace {} {

class Simple : public cSimpleModule
{
  public:
    Simple() : cSimpleModule(16384) { }
    virtual void activity();
};

Define_Module(Simple);

void Simple::activity()
{
    ev << strlen((const char *)par("verylongstring")) << endl;
    ev << (const char *)par("verylongstring") << endl;
    ev << "." << endl;
}

}; //namespace


%inifile: test.ini
[General]
network = Simple

