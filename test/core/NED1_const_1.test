%description:
Test that const parameters in NED are really converted to constant.

%file: test.ned

simple Simple
{
    parameters:
        double p0;
        double p1;
        double p2;
        double p3;
}

network Test extends Simple
{
    parameters:
        p0 = truncnormal(10, 3);
        p1 = 1+truncnormal(10, 3);
        p2 = exponential(1);
        p3 = 1+exponential(1);
}

%file: test.cc

#include <omnetpp.h>

namespace {} {

class Simple : public cSimpleModule
{
  public:
    Simple() : cSimpleModule(16384) { }
    virtual void activity();
};

Define_Module(Simple);

void Simple::activity()
{
    for (int i=0; i<params(); i++)
        ev << par(i).name() << ":" << (par(i).isExpression()?"expr":"constant") << endl;
}

}; //namespace

%contains: stdout
p0:constant
p1:constant
p2:constant
p3:constant

