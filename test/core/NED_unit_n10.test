%description:
Negative test for expressions that they do not have units in NED.

%file: test.ned

simple Simple
{
    parameters:
        double p1 = floor(10.2ms) + 0.5 @unit(s); // expression units are not supported
}

network Test
{
    submodules:
        a: Simple;
}

%file: test.cc

#include <omnetpp.h>

namespace {} {

class Simple : public cSimpleModule
{
  public:
    Simple() : cSimpleModule(16384) { }
    virtual void activity();
};

Define_Module(Simple);

void Simple::activity()
{
   ev << "This should not be printed: " << par("p1").doubleValue() << "\n";
}

}; //namespace

%exitcode: 1

%contains: stdout
