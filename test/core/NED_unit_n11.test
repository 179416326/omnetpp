%description:
Negative test for a value with multiple units in NED.

%file: test.ned

simple Simple
{
    parameters:
        double p1 = 1h 22s 550ms @unit(s); // values may not have multiple units
}

network Test
{
    submodules:
        a: Simple;
}

%file: test.cc

#include <omnetpp.h>

namespace {} {

class Simple : public cSimpleModule
{
  public:
    Simple() : cSimpleModule(16384) { }
    virtual void activity();
};

Define_Module(Simple);

void Simple::activity()
{
   ev << "This should not be printed: " << par("p1").doubleValue() << "\n";
}

}; //namespace

%exitcode: 1

%contains: stdout
