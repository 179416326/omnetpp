%description:
Check inheritance for messages

%file: test.msg

namespace {};

message Base
{
    fields:
        int b;
}

message MyMessage extends Base
{
    fields:
        char c;
};

%includes:
#include "test_m.h"

%activity:

#define PRINT(X) ev << #X << ":" << X << endl

MyMessage x("msg");

ev << x.className() << ":" << x.name() << endl;
x.setB(11); PRINT(x.getB());
x.setC('x'); PRINT(x.getC());

%contains: stdout
MyMessage:msg
x.getB():11
x.getC():x

