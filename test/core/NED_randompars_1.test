%description:
Test random module parameters

%file: {}.ned

simple {Simple}
   parameters:
      p0, p1, c0:const, c1:const;
endsimple

network {network}: {Simple}
   parameters:
      p0 = truncnormal(10,3),
      p1 = 1+truncnormal(10,3),
      c0 = exponential(1),
      c1 = 1+exponential(1);
endnetwork

%file: {}.cc

#include <omnetpp.h>

class {Simple} : public cSimpleModule
{
    Module_Class_Members({Simple},cSimpleModule,16384)
    virtual void activity();
};

Define_Module({Simple});

void {Simple}::activity()
{
    for (int i=0; i<params(); i++)
        ev << par(i).name() << ":" << par(i).type() << ":" << par(i).doubleValue() << "," << par(i).doubleValue() << endl;
}

%contains-regex: stdout
p0:[XFC]:11\.89\d+,11\.371\d+
p1:[XFC]:8\.194\d+,12\.896\d+
c0:D:0\.599\d+,0\.599\d+
c1:D:1\.522\d+,1\.522\d+

