%description:
Check struct as struct member

%file: test.msg

struct Base
{
    fields:
        int bb;
}


struct MyStruct
{
    fields:
        Base b;
        char c;
};

%global:
#include "test_m.h"

%activity:

#define PRINT(X) ev << #X << ":" << X << endl

MyStruct x;

x.b.bb = 11; PRINT(x.b.bb);
x.c = 'x'; PRINT(x.c);

%contains: stdout
x.b.bb:11
x.c:x

