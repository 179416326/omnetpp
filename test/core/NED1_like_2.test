%description:
"like" keyword: check what if given module doesn't exist

%file: test.ned

moduleinterface IFoo
{
}

network Test
{
    parameters:
        string modType;
    submodules:
        specMod: <modType> like IFoo;
}

%inifile: test.ini
[General]
network = Test
cmdenv-express-mode = false
*.modType = "bla-bla-bla"

%exitcode: 1

%contains-regex: stdout
type definition .*bla-bla-bla.* not found

