%description:
"like" keyword: check what if given module doesn't exist

%file: test.ned

module GenMod
{
}

network Test
{
    parameters:
        string mod_type;
    submodules:
        specMod: <mod_type> like GenMod;
}

%inifile: test.ini
[General]
network={}.Test
warnings=yes
cmdenv-express-mode=false
*.mod_type = "bla-bla-bla"

%exitcode: 1

%contains-regex: stdout
type definition .*bla-bla-bla.* not found

