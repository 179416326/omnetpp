%description:
Test initialization of the RNGs.

%activity:

ev << endl;
for (int i=0; i<32; i++)
{
    ev << "RNG" << i << ": " << genk_randseed(i) << endl;
}
ev << endl;

%inifile: {}.ini
[General]
network={}_network
# random-seed is the same as gen0-seed
random-seed=9000
# default seeds for both Run1 and Run2
gen1-seed=9001
gen2-seed=9002
gen3-seed=9003
gen4-seed=9004

[Run 1]
# override seed-2 in [General]
gen2-seed=1002
# new seeds - indexes common with Run 2
gen7-seed=1007
gen8-seed=1008
gen9-seed=1009
# seeds not in General or Run 2
gen15-seed=1015
gen28-seed=1028
# remaining seeds (0,5,6,10,11,..) should be initialized from table

[Run 2]
# override seed-0 and seed-3 in [General] (random-seed is same as gen0-seed)
random-seed=2000
gen3-seed=2003
# new seeds - indexes common with Run 1
gen7-seed=2007
gen8-seed=2008
gen9-seed=2009
# seeds not in General or Run 1
gen19-seed=2019
gen22-seed=2022
# remaining seeds (0,5,6,10,11,..) should be initialized from table,
# not repeating seeds used in Run 1

