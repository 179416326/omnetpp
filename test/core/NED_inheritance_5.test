%description:

Test parameter inheritance for channel interfaces.

%file: test.ned

channelinterface IChannel
{
    parameters:
        int p1;
        double p2;
}

channel TestChannel extends ned.IdealChannel like IChannel
{
    parameters:
        int p1;
        double p2;
}

module Node
{
    gates:
        inout g;
    connections allowunconnected:
}

network Test
{
    parameters:
        string aType = "TestChannel";
    submodules:
        a: Node;
    connections:
        a.g$o --> <aType> like IChannel { parameters: p1 = 1; p2 = 2; }--> a.g$i;
}

%file: test.cc

