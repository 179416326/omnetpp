%description:
dup() of cLongHistogram

%activity:

FILE *f = fopen("histograms.out", "w");
if (!f)
    throw cRuntimeError("cannot open output file for histograms");

cLongHistogram h;
cLongHistogram *h2;

h.setNumCells(10);   // 10 cells
h.setNumFirstVals(5);

h.collect(10);
h.collect(19);
h.collect(9);

h.saveToFile(f);
h2 = h.dup(); h2->saveToFile(f); delete h2;

h.collect(10);
h.collect(12);
h.collect(12);
h.collect(20);

h.saveToFile(f);
h2 = h.dup(); h2->saveToFile(f); delete h2;

fclose(f);


