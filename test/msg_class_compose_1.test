%description:
Check struct as class member

%file: {}.msg

struct {MyStruct}
{
    fields:
        int bb;
}


class {MyClass} extends cObject
{
    fields:
        {MyStruct} b;
        char c;
};

%global:
#include "{}_m.h"

%activity:

#define PRINT(X) ev << #X << ":" << X << endl

{MyClass} x;

x.getB().bb=11; PRINT(x.getB().bb);
x.setC('x'); PRINT(x.getC());

%contains: stdout
x.getB().bb:11
x.getC():x

